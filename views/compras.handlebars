<a href="/"><button type="button" class="btn btn-success">Voltar</button></a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Situação Do imóvel</th>
            <th scope="col">Tipo de imóvel</th>
            <th scope="col">Local</th>
            <th scope="col">Nome do proprietario</th>
            <th scope="col">Valor do imóvel</th>
          
        </tr>
    </thead>
    <tbody>
        {{#each imoveis}}
        <tr>
            <th>{{id}}</th>
            <td>{{situacao}}</td>
            <td>{{tipoDeImovel}}</td>
            <td>{{local}}</td>
            <td>{{nomeDoProprietario}}</td>
            <td>{{valor}}</td>
          
        </tr>
        {{/each}}
    </tbody>
</table>
